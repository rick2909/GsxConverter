	"version": "0.2.0",
	"configurations": [
		{
			"name": ".NET: GsxConverter (Console)",
			"type": "dotnet",
			"request": "launch",
			"projectPath": "${workspaceFolder}/GsxConverter/GsxConverter.csproj",
			"preLaunchTask": "build-console"
		},
		{
			"name": ".NET MAUI: UI.Maui (Windows)",
			"type": "dotnet",
			"request": "launch",
			"projectPath": "${workspaceFolder}/UI.Maui/UI.Maui.csproj",
			"preLaunchTask": "build-maui"
		},
		{
			"name": "Attach to Process",
			"type": "dotnet",
			"request": "attach",
			"processId": "${command:pickProcess}"
		}
	]
}